{
  "name": "lanventory",
  "version": "0.1.2",
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "stream": false,
    "child_process": false
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:migrate": "node -r esbuild-register ./src/db/migrate.ts",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@libsql/client": "0.14.0",
    "@network-utils/arp-lookup": "^2.1.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "bonjour-service": "^1.3.0",
    "child_process": "^1.0.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "0.40.0",
    "drizzle-zod": "^0.7.0",
    "fast-xml-parser": "^5.0.8",
    "hostile": "^1.4.0",
    "ip-num": "^1.5.1",
    "node-cron": "^3.0.3",
    "node-nmap-hosts": "^0.1.2",
    "path": "^0.12.7",
    "radash": "^12.1.0",
    "resend": "4.1.2",
    "server-only": "^0.0.1",
    "ssh-config": "^5.0.3",

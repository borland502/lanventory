    "@types/xml2js": "^0.4.14",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/ui": "^3.1.2",
    "commander": "^13.1.0",
    "drizzle-kit": "0.30.5",
    "esbuild-register": "3.6.0",
    "eslint": "9.22.0",
    "eslint-config-next": "15.2.2",
    "path-browserify": "^1.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "5.8.2",
